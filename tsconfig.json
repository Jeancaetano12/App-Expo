{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@contexts/*": ["src/contexts/*"],
      "@config/*": ["src/config/*"],
      "@services/*": ["src/services/*"],
      "@styles/*": ["src/styles/*"],
      "@assets/*": ["assets/*"]
    }
  }
}